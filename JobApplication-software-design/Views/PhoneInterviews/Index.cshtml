@model IEnumerable<JobApplication_software_design.Models.PhoneInterview>

@{
    ViewData["Title"] = "Index";
}

<h1>Phone Interviews</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DialInNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobApplication)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Interview)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneInterviewId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InPersonInterviewId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DialInNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobApplication.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Interview.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneInterviewId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InPersonInterviewId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
